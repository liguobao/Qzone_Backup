{
    "manifest_version":2,
	"name":"QQ空间备份插件",
	"version":"0.0.0",
	"description":"QQ空间备份插件",
	"icons":
	{
	   "16":"images/icon16.png",
	   "48":"images/icon48.png",
	   "128":"images/icon128.png"
	},
	"browser_action": 
	{
	   "default_icon":
	   {
	      "19":"images/icon19.png",
		  "38":"images/icon38.png"
	   },
	   "default_title":"QQ空间备份",
	   "default_popup":"popup.html"
	},
	"content_scripts":[{
		"matches":["http://user.qzone.qq.com/*","https://user.qzone.qq.com/*"],
		"js":["jquery-2.0.0.min.js","myscript.js"],
		"run_at":"document_end"
	}]
	
}